# =============================================================================
# Ontologia degli Elenchi Pubblicati (PublishedRegistry)
# Esempio completo con definizione classi e istanze di esempio
# =============================================================================

@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix dct:    <http://purl.org/dc/terms/> .

# Ontologie Italia esistenti
@prefix l0:     <https://w3id.org/italia/onto/l0/> .
@prefix TI:     <https://w3id.org/italia/onto/TI/> .
@prefix COV:    <https://w3id.org/italia/onto/COV/> .
@prefix CPV:    <https://w3id.org/italia/onto/CPV/> .
@prefix CLV:    <https://w3id.org/italia/onto/CLV/> .
@prefix CPEV:   <https://w3id.org/italia/onto/CPEV/> .

# Ontologia esistente albi professionali
@prefix ssn:    <https://w3id.org/italia/social-security/onto/socialSafetyNet/> .

# Nuova ontologia proposta
@prefix PREG:   <https://w3id.org/italia/onto/PublishedRegistry/> .


# =============================================================================
# DEFINIZIONE ONTOLOGIA
# =============================================================================

<https://w3id.org/italia/onto/PublishedRegistry>
    a owl:Ontology ;
    owl:versionInfo "0.1.0" ;
    dct:title "Ontologia degli Elenchi Pubblicati"@it ;
    dct:title "Published Registry Ontology"@en ;
    dct:description """Ontologia per la modellazione di elenchi e registri
        pubblicati da organizzazioni. Generalizza il concetto di Albo Professionale
        per coprire elenchi di persone, oggetti materiali e immateriali."""@it ;
    dct:creator "Schema.gov.it Community" ;
    dct:issued "2024-01-01"^^xsd:date ;
    owl:imports <https://w3id.org/italia/onto/l0> ;
    owl:imports <https://w3id.org/italia/onto/TI> ;
    owl:imports <https://w3id.org/italia/onto/COV> .


# =============================================================================
# CLASSI PRINCIPALI
# =============================================================================

# -----------------------------------------------------------------------------
# PublishedRegistry - Registro/Elenco Pubblicato
# -----------------------------------------------------------------------------

PREG:PublishedRegistry
    a owl:Class ;
    rdfs:subClassOf l0:Collection ;
    rdfs:label "Registro Pubblicato"@it ;
    rdfs:label "Published Registry"@en ;
    rdfs:comment """Un elenco o registro ufficialmente pubblicato da un'organizzazione.
        Può contenere persone, organizzazioni, beni, eventi o altri elementi.
        Esempi: albo professionale, libro soci, graduatoria concorso, inventario beni."""@it ;
    rdfs:comment """A list or registry officially published by an organization.
        May contain persons, organizations, assets, events or other items.
        Examples: professional register, membership roll, competition ranking, asset inventory."""@en ;
    owl:versionInfo "stabile"@it .

# -----------------------------------------------------------------------------
# RegistryEntry - Voce del Registro
# -----------------------------------------------------------------------------

PREG:RegistryEntry
    a owl:Class ;
    rdfs:subClassOf TI:TimeIndexedEvent ;
    rdfs:label "Voce del Registro"@it ;
    rdfs:label "Registry Entry"@en ;
    rdfs:comment """Una singola voce/iscrizione in un registro pubblicato.
        Rappresenta l'evento di inserimento di un'entità nel registro,
        con validità temporale (data inizio, eventuale data fine)."""@it ;
    rdfs:comment """A single entry/registration in a published registry.
        Represents the event of inserting an entity into the registry,
        with temporal validity (start date, optional end date)."""@en ;
    owl:versionInfo "stabile"@it .

# -----------------------------------------------------------------------------
# RegistrySection - Sezione del Registro
# -----------------------------------------------------------------------------

PREG:RegistrySection
    a owl:Class ;
    rdfs:subClassOf l0:Collection ;
    rdfs:label "Sezione del Registro"@it ;
    rdfs:label "Registry Section"@en ;
    rdfs:comment """Suddivisione interna di un registro pubblicato.
        Esempi: Sezione A/B di un albo, categorie di soci, fasce di graduatoria."""@it ;
    owl:versionInfo "stabile"@it .


# =============================================================================
# SPECIALIZZAZIONI
# =============================================================================

# -----------------------------------------------------------------------------
# Tipi di Registro
# -----------------------------------------------------------------------------

PREG:ProfessionalRegister
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Albo Professionale"@it ;
    rdfs:label "Professional Register"@en ;
    rdfs:comment """Registro in cui sono raccolti i nomi e i dati di tutte
        le persone abilitate a esercitare una professione regolamentata."""@it ;
    owl:equivalentClass ssn:AlboProfessionale .

PREG:MembershipRoll
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Libro Soci"@it ;
    rdfs:label "Membership Roll"@en ;
    rdfs:comment """Registro contenente l'elenco dei soci di un'associazione,
        cooperativa o altra organizzazione associativa."""@it .

PREG:SelectionList
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Graduatoria/Elenco Ammessi"@it ;
    rdfs:label "Selection List"@en ;
    rdfs:comment """Elenco ordinato di persone ammesse a una selezione,
        concorso o procedura comparativa."""@it .

PREG:QualifiedSupplierList
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Elenco Operatori Qualificati"@it ;
    rdfs:label "Qualified Supplier List"@en ;
    rdfs:comment """Elenco di operatori economici qualificati per
        l'affidamento di lavori, servizi o forniture."""@it .

PREG:AssetRegistry
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Registro Beni"@it ;
    rdfs:label "Asset Registry"@en ;
    rdfs:comment """Inventario o registro di beni materiali o immateriali
        di proprietà o in gestione a un'organizzazione."""@it .

PREG:EventCalendar
    a owl:Class ;
    rdfs:subClassOf PREG:PublishedRegistry ;
    rdfs:label "Calendario Eventi"@it ;
    rdfs:label "Event Calendar"@en ;
    rdfs:comment """Elenco pubblicato di eventi, manifestazioni o
        appuntamenti programmati."""@it .

# -----------------------------------------------------------------------------
# Tipi di Voce
# -----------------------------------------------------------------------------

PREG:ProfessionalRegistration
    a owl:Class ;
    rdfs:subClassOf PREG:RegistryEntry ;
    rdfs:label "Iscrizione Albo Professionale"@it ;
    rdfs:label "Professional Registration"@en ;
    owl:equivalentClass ssn:IscrizioneAlboProfessionale .

PREG:MembershipEntry
    a owl:Class ;
    rdfs:subClassOf PREG:RegistryEntry ;
    rdfs:label "Iscrizione Socio"@it ;
    rdfs:label "Membership Entry"@en .

PREG:AdmissionEntry
    a owl:Class ;
    rdfs:subClassOf PREG:RegistryEntry ;
    rdfs:label "Ammissione"@it ;
    rdfs:label "Admission Entry"@en .

PREG:AssetRegistration
    a owl:Class ;
    rdfs:subClassOf PREG:RegistryEntry ;
    rdfs:label "Registrazione Bene"@it ;
    rdfs:label "Asset Registration"@en .


# =============================================================================
# OBJECT PROPERTIES
# =============================================================================

PREG:pubblicatoDa
    a owl:ObjectProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range COV:Organization ;
    rdfs:label "pubblicato da"@it ;
    rdfs:label "published by"@en ;
    rdfs:comment "L'organizzazione che pubblica e gestisce il registro."@it .

PREG:haSezione
    a owl:ObjectProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range PREG:RegistrySection ;
    rdfs:label "ha sezione"@it ;
    rdfs:label "has section"@en ;
    rdfs:comment "Collega un registro alle sue sezioni/suddivisioni."@it .

PREG:sezioneDelRegistro
    a owl:ObjectProperty ;
    owl:inverseOf PREG:haSezione ;
    rdfs:domain PREG:RegistrySection ;
    rdfs:range PREG:PublishedRegistry ;
    rdfs:label "sezione del registro"@it ;
    rdfs:label "section of registry"@en .

PREG:haVoce
    a owl:ObjectProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range PREG:RegistryEntry ;
    rdfs:label "ha voce"@it ;
    rdfs:label "has entry"@en ;
    rdfs:comment "Collega un registro alle voci/iscrizioni contenute."@it .

PREG:inRegistro
    a owl:ObjectProperty ;
    owl:inverseOf PREG:haVoce ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range PREG:PublishedRegistry ;
    rdfs:label "in registro"@it ;
    rdfs:label "in registry"@en .

PREG:inSezione
    a owl:ObjectProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range PREG:RegistrySection ;
    rdfs:label "in sezione"@it ;
    rdfs:label "in section"@en .

PREG:riferimentoA
    a owl:ObjectProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range l0:Entity ;
    rdfs:label "riferimento a"@it ;
    rdfs:label "refers to"@en ;
    rdfs:comment "L'entità (persona, organizzazione, bene, evento) oggetto dell'iscrizione."@it .

PREG:tipoContenuto
    a owl:ObjectProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range PREG:RegistryContentType ;
    rdfs:label "tipo contenuto"@it ;
    rdfs:label "content type"@en .

PREG:tipoRegistro
    a owl:ObjectProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range PREG:RegistryType ;
    rdfs:label "tipo registro"@it ;
    rdfs:label "registry type"@en .

PREG:stato
    a owl:ObjectProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range PREG:EntryStatus ;
    rdfs:label "stato"@it ;
    rdfs:label "status"@en .


# =============================================================================
# DATATYPE PROPERTIES
# =============================================================================

PREG:nome
    a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf (PREG:PublishedRegistry PREG:RegistrySection) ] ;
    rdfs:range xsd:string ;
    rdfs:label "nome"@it ;
    rdfs:label "name"@en .

PREG:descrizione
    a owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:label "descrizione"@it ;
    rdfs:label "description"@en .

PREG:baseNormativa
    a owl:DatatypeProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range xsd:string ;
    rdfs:label "base normativa"@it ;
    rdfs:label "legal basis"@en ;
    rdfs:comment "Riferimento alla norma che istituisce il registro."@it .

PREG:dataIstituzione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:PublishedRegistry ;
    rdfs:range xsd:date ;
    rdfs:label "data istituzione"@it ;
    rdfs:label "establishment date"@en .

PREG:identificativoVoce
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:string ;
    rdfs:label "identificativo voce"@it ;
    rdfs:label "entry identifier"@en ;
    rdfs:comment "Numero o codice univoco della voce nel registro."@it .

PREG:dataIscrizione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:date ;
    rdfs:label "data iscrizione"@it ;
    rdfs:label "registration date"@en .

PREG:dataCessazione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:date ;
    rdfs:label "data cessazione"@it ;
    rdfs:label "termination date"@en .

PREG:motivoIscrizione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:string ;
    rdfs:label "motivo iscrizione"@it ;
    rdfs:label "registration reason"@en .

PREG:motivoCessazione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:string ;
    rdfs:label "motivo cessazione"@it ;
    rdfs:label "termination reason"@en .

PREG:annotazioni
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:string ;
    rdfs:label "annotazioni"@it ;
    rdfs:label "notes"@en .

PREG:posizione
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:integer ;
    rdfs:label "posizione"@it ;
    rdfs:label "position"@en ;
    rdfs:comment "Posizione in graduatoria (per SelectionList)."@it .

PREG:punteggio
    a owl:DatatypeProperty ;
    rdfs:domain PREG:RegistryEntry ;
    rdfs:range xsd:decimal ;
    rdfs:label "punteggio"@it ;
    rdfs:label "score"@en .


# =============================================================================
# VOCABOLARI CONTROLLATI
# =============================================================================

# -----------------------------------------------------------------------------
# RegistryContentType - Tipo di Contenuto
# -----------------------------------------------------------------------------

PREG:RegistryContentType
    a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Tipo di Contenuto del Registro"@it .

PREG:ContentType-Person a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Persone"@it, "Persons"@en ;
    skos:definition "Il registro contiene persone fisiche."@it .

PREG:ContentType-Organization a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Organizzazioni"@it, "Organizations"@en ;
    skos:definition "Il registro contiene enti e organizzazioni."@it .

PREG:ContentType-RealEstate a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Beni Immobili"@it, "Real Estate"@en ;
    skos:definition "Il registro contiene beni immobili."@it .

PREG:ContentType-MovableAsset a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Beni Mobili"@it, "Movable Assets"@en ;
    skos:definition "Il registro contiene beni mobili."@it .

PREG:ContentType-Event a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Eventi"@it, "Events"@en ;
    skos:definition "Il registro contiene eventi."@it .

PREG:ContentType-Service a PREG:RegistryContentType, skos:Concept ;
    skos:prefLabel "Servizi"@it, "Services"@en ;
    skos:definition "Il registro contiene servizi."@it .

# -----------------------------------------------------------------------------
# EntryStatus - Stato della Voce
# -----------------------------------------------------------------------------

PREG:EntryStatus
    a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Stato della Voce"@it .

PREG:Status-Active a PREG:EntryStatus, skos:Concept ;
    skos:prefLabel "Attivo"@it, "Active"@en ;
    skos:definition "La voce è attiva e vigente."@it .

PREG:Status-Suspended a PREG:EntryStatus, skos:Concept ;
    skos:prefLabel "Sospeso"@it, "Suspended"@en ;
    skos:definition "La voce è temporaneamente sospesa."@it .

PREG:Status-Cancelled a PREG:EntryStatus, skos:Concept ;
    skos:prefLabel "Cancellato"@it, "Cancelled"@en ;
    skos:definition "La voce è stata cancellata dal registro."@it .

PREG:Status-Expired a PREG:EntryStatus, skos:Concept ;
    skos:prefLabel "Scaduto"@it, "Expired"@en ;
    skos:definition "La voce è scaduta per decorso dei termini."@it .

PREG:Status-Pending a PREG:EntryStatus, skos:Concept ;
    skos:prefLabel "In Attesa"@it, "Pending"@en ;
    skos:definition "La voce è in attesa di conferma/validazione."@it .

# -----------------------------------------------------------------------------
# RegistryType - Tipo di Registro
# -----------------------------------------------------------------------------

PREG:RegistryType
    a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Tipo di Registro"@it .

PREG:Type-ProfessionalRegister a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Albo Professionale"@it, "Professional Register"@en .

PREG:Type-MembershipRoll a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Libro Soci"@it, "Membership Roll"@en .

PREG:Type-SelectionList a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Graduatoria"@it, "Selection List"@en .

PREG:Type-QualifiedSupplierList a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Elenco Fornitori Qualificati"@it, "Qualified Supplier List"@en .

PREG:Type-PublicInventory a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Inventario Pubblico"@it, "Public Inventory"@en .

PREG:Type-EventCalendar a PREG:RegistryType, skos:Concept ;
    skos:prefLabel "Calendario Eventi"@it, "Event Calendar"@en .


