    skos:prefLabel "Beni Mobili"@it , "Movable Assets"@en ;
    skos:definition "Il registro contiene beni mobili."@it ;
    skos:definition "The registry contains movable assets."@en .

PREG:ContentType-Event a PREG:RegistryContentType , skos:Concept ;
    skos:inScheme <https://w3id.org/italia/controlled-vocabulary/PREG/registry-content-type> ;
    skos:prefLabel "Eventi"@it , "Events"@en ;
    skos:definition "Il registro contiene eventi."@it ;
    skos:definition "The registry contains events."@en .

PREG:ContentType-Service a PREG:RegistryContentType , skos:Concept ;
    skos:inScheme <https://w3id.org/italia/controlled-vocabulary/PREG/registry-content-type> ;
    skos:prefLabel "Servizi"@it , "Services"@en ;
    skos:definition "Il registro contiene servizi."@it ;
    skos:definition "The registry contains services."@en .


# -----------------------------------------------------------------------------
# RecordStatus - Stato della Voce
# -----------------------------------------------------------------------------

<https://w3id.org/italia/controlled-vocabulary/PREG/record-status>
    a skos:ConceptScheme ;
